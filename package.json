{
  "name": "balcora-bot",
  "version": "0.0.1",
  "description": "Discord bot for quickly fetching HWRM data",
  "main": "main.js",
  "scripts": {
    "build": "npm install && tsc",
    "rebuild": "rm -rf ./node_modules && npm run build",
    "quickstart": "node ./main.js --trace-warnings",
    "qs": "npm run quickstart",
    "start": "npm run build && npm run quickstart"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Novaras/balcora-bot.git"
  },
  "keywords": [
    "Homeworld",
    "Homeworld Remastered",
    "Discord",
    "Balcora",
    "CLI",
    "Data",
    "Reference"
  ],
  "author": "Novaras",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Novaras/balcora-bot/issues"
  },
  "homepage": "https://github.com/Novaras/balcora-bot#readme",
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/mongodb": "^3.5.25",
    "@types/node": "^14.0.23",
    "@types/node-fetch": "^2.5.7",
    "@types/winston": "^2.4.4",
    "@types/ws": "^7.2.6",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "mongodb": "^3.5.9",
    "node-fetch": "^3.1.1",
    "winston": "^3.3.3"
  }
